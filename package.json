  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/puppeteer": "^5.4.2",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "puppeteer": "^5.5.0"
  }
